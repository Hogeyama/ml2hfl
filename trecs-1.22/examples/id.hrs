%BEGING
S -> F 0.
F x -> Visit x Id1 leaf.
Id1 l x1 x2 -> Label l (F x1) (F x2).
Label l x y ->
  _case 2 l
    (doc x y)
    (a x y).
Visit x f1 f2 ->
  _case 4 x
    (f1 0 1 3)
    (f1 1 2 3)
    (br2 (f1 1 2 2) f2)
    f2.
%ENDG

%BEGINA
q0 doc -> q1 q3.
q0 br2 -> q0 q0.
q1 a -> q2 q3.
q1 br2 -> q1 q1.
q2 a -> q2 q2.
q2 leaf -> .
q2 br2 -> q2 q2.
q3 leaf -> .
q3 br2 -> q3 q3.
%ENDA
