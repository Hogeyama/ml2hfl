%BEGING
S -> Add (1, (1, (0, 0))) (1, (1, (0, 0))).
Add x y -> Addsub1 Copy x y.
Addsub1 k x y ->
  _dcons x (Addsub2 k y).
Addsub2 k y c x ->
  _case 2 c
    /* zero */ (k y)
    /* s(x) */
      (Addsub1 k x (1,y)).
Copy x -> _dcons x G.
G x y -> _case 2 x zero (s (Copy y)).
%ENDG

%BEGINA
q0 s -> q1.
q1 s -> q0.
q0 zero -> .
%ENDA

