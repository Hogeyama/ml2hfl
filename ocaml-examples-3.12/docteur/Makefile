# Makefile generated by imake - do not edit!

CPP=/lib/cpp -P
RM=rm -f
CP=cp -p -f
TOPDIR=.
CONFIGDIR=$(TOPDIR)/config

LIBDIR=/usr/local/lib/ocaml/

CAMLCOMPILERBIN=ocamlopt
CAMLCOMPILERBYT=ocamlc
CAMLFLAGSBIN=-w A -warn-error Ae
CAMLFLAGSBYT=-g $(CAMLFLAGSBIN)

CAMLBIN=$(CAMLCOMPILERBIN) $(CAMLFLAGSBIN)
CAMLBYT=$(CAMLCOMPILERBYT) $(CAMLFLAGSBYT)

CAMLLIBRBYT=ocamlmklib
CAMLLIBRBIN=ocamlmklib

CAMLLEX=ocamllex
CAMLYACC=ocamlyacc
CAMLDEP=ocamldep -pp camlp4o

all:: byt bin

# (begin Imakefile)

docteur.BYTOBJS=docteur.cmo
docteur.FILES=$(docteur.BYTOBJS:.cmo=.ml)

byt:: docteur.byt

depend::
	$(CAMLDEP) $(docteur.FILES) >> Makefile

clean::
	$(RM) docteur.byt

docteur.byt: docteur.cmo
	$(CAMLBYT) $(LINKFLAGSBYT)  docteur.cmo -o docteur.byt
	$(CP) docteur.byt docteur

clean::
	$(RM) docteur

docteur.BINOBJS=docteur.cmx
docteur.FILES=$(docteur.BINOBJS:.cmx=.ml)

bin:: docteur.bin

depend::
	$(CAMLDEP) $(docteur.FILES) >> Makefile

clean::
	$(RM) docteur.bin

docteur.bin: docteur.cmx
	$(CAMLBIN) $(LINKFLAGSBIN)  docteur.cmx -o docteur.bin
	$(CP) docteur.bin docteur

clean::
	$(RM) docteur

# (end Imakefile)

clean::
	$(RM) *.cmi *.cmix *.cmo *.cmx *.o a.out* *~ *.byt *.bin #*#

depend::

beforedepend::

mfile:
