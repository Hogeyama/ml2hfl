	$(CAMLDEP) $(cfastgrep.FILES) >> Makefile

clean::
	$(RM) cfastgrep.byt

cfastgrep.byt: expr.cmo auto.cmo ensent.cmo determ.cmo fastgrep.cmo fastrec.o
	$(CAMLBYT) $(LINKFLAGSBYT) -custom  expr.cmo auto.cmo ensent.cmo determ.cmo fastgrep.cmo fastrec.o -o cfastgrep

fastrec.o: fastrec.c
	$(CAMLBYT) -c -I $(LIBDIR)/caml fastrec.c

clean::
	rm -f fastrec.o

