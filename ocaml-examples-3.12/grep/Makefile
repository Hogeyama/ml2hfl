CAMLCOMPILERBYT=ocamlc
CAMLFLAGSBIN=-w A -warn-error Ae
CAMLFLAGSBYT=-g $(CAMLFLAGSBIN)

CAMLBIN=$(CAMLCOMPILERBIN) $(CAMLFLAGSBIN)
CAMLBYT=$(CAMLCOMPILERBYT) $(CAMLFLAGSBYT)

CAMLLIBRBYT=ocamlmklib
CAMLLIBRBIN=ocamlmklib

CAMLLEX=ocamllex
CAMLYACC=ocamlyacc
CAMLDEP=ocamldep -pp camlp4o

all:: byt bin

# (begin Imakefile)

CAMLFLAGSBYT=-w A -warn-error Ae -g -pp camlp4o

grep.BYTOBJS=expr.cmo auto.cmo ensent.cmo determ.cmo grep.cmo
grep.FILES=$(grep.BYTOBJS:.cmo=.ml)

byt:: grep.byt

depend::
	$(CAMLDEP) $(grep.FILES) >> Makefile

clean::
	$(RM) grep.byt

grep.byt: expr.cmo auto.cmo ensent.cmo determ.cmo grep.cmo
	$(CAMLBYT) $(LINKFLAGSBYT)  expr.cmo auto.cmo ensent.cmo determ.cmo grep.cmo -o grep

CAMLFLAGSBIN=-w A -warn-error Ae -pp camlp4o

fastgrep.BINOBJS=expr.cmx auto.cmx ensent.cmx determ.cmx grep.cmx
fastgrep.FILES=$(fastgrep.BINOBJS:.cmx=.ml)

