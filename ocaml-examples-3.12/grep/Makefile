	$(CAMLDEP) $(fastgrep.FILES) >> Makefile

clean::
	$(RM) fastgrep.bin

fastgrep.bin: expr.cmx auto.cmx ensent.cmx determ.cmx grep.cmx
	$(CAMLBIN) $(LINKFLAGSBIN)  expr.cmx auto.cmx ensent.cmx determ.cmx grep.cmx -o fastgrep.bin
	$(CP) fastgrep.bin fastgrep

cfastgrep.BYTOBJS=expr.cmo auto.cmo ensent.cmo determ.cmo fastgrep.cmo fastrec.o
cfastgrep.FILES=$(cfastgrep.BYTOBJS:.cmo=.ml)

byt:: cfastgrep.byt

