# Makefile generated by imake - do not edit!

CPP=/lib/cpp -P
RM=rm -f
CP=cp -p -f
TOPDIR=.
CONFIGDIR=$(TOPDIR)/config

LIBDIR=/usr/local/lib/ocaml/

CAMLCOMPILERBIN=ocamlopt
CAMLCOMPILERBYT=ocamlc
CAMLFLAGSBIN=-w A -warn-error Ae
CAMLFLAGSBYT=-g $(CAMLFLAGSBIN)

CAMLBIN=$(CAMLCOMPILERBIN) $(CAMLFLAGSBIN)
CAMLBYT=$(CAMLCOMPILERBYT) $(CAMLFLAGSBYT)

CAMLLIBRBYT=ocamlmklib
CAMLLIBRBIN=ocamlmklib

CAMLLEX=ocamllex
CAMLYACC=ocamlyacc
CAMLDEP=ocamldep -pp camlp4o

all:: byt bin

# (begin Imakefile)

CAMLFLAGSBYT=-g -w A -warn-error Ae -pp camlp4o

logo.BYTOBJS=crayon.cmo langage.cmo alex.cmo asynt.cmo logo.cmo
logo.FILES=$(logo.BYTOBJS:.cmo=.ml)

byt:: logo.byt

depend::
	$(CAMLDEP) $(logo.FILES) >> Makefile

clean::
	$(RM) logo.byt

logo.byt: crayon.cmo langage.cmo alex.cmo asynt.cmo logo.cmo
	$(CAMLBYT) $(LINKFLAGSBYT) -custom unix.cma graphics.cma  crayon.cmo langage.cmo alex.cmo asynt.cmo logo.cmo -o logo

CAMLFLAGSBIN=-w A -warn-error Ae -pp camlp4o

logo.BINOBJS=crayon.cmx langage.cmx alex.cmx asynt.cmx logo.cmx
logo.FILES=$(logo.BINOBJS:.cmx=.ml)

bin:: logo.bin

depend::
	$(CAMLDEP) $(logo.FILES) >> Makefile

clean::
	$(RM) logo.bin

logo.bin: crayon.cmx langage.cmx alex.cmx asynt.cmx logo.cmx
	$(CAMLBIN) $(LINKFLAGSBIN) unix.cmxa graphics.cmxa  crayon.cmx langage.cmx alex.cmx asynt.cmx logo.cmx -o logo.bin
	$(CP) logo.bin logo

# (end Imakefile)

clean::
	$(RM) *.cmi *.cmix *.cmo *.cmx *.o a.out* *~ *.byt *.bin #*#

depend::

beforedepend::

mfile:
	imake -I$(CONFIGDIR)

makefiles:: beforedepend mfile depend

.SUFFIXES: .mli .cmi .ml .mly .mll .cmo .cmx .cma .cmxa

.mli.cmi:
	$(CAMLBYT) -c $<

.ml.cmo:
	$(CAMLBYT) -c $<

.ml.cmx:
	$(CAMLBIN) -c $<

