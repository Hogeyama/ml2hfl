# A simple script to compile the Sudoku game.

# Types.ml is just a link to types.mli
if ! (test -e types.ml); then
  ln -s types.mli types.ml;
fi

ocamlc -c types.mli
ocamlc -c types.ml
ocamlc -c utils.ml
ocamlc -c solve.ml
ocamlc -c resolve.ml
ocamlc -c read.ml
ocamlc -c main.ml
ocamlc -o sudoku.byt types.cmo utils.cmo solve.cmo resolve.cmo read.cmo main.cmo

rm -f ./sudoku
cp -p ./sudoku.byt ./sudoku
